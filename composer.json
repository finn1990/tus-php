    "guzzlehttp/guzzle": "^6.3",
    "predis/predis": "^1.1",
    "symfony/console": "^2.8",
    "ramsey/uuid": "^3.7",
    "nesbot/carbon": "^1.26"
  },
  "require-dev": {
    "phpunit/phpunit": "6.5.x-dev",
